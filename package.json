{
  "name": "@hypernym/merge",
  "version": "0.3.1",
  "author": "Hypernym Studio",
  "description": "Type-safe deep merge utility.",
  "license": "MIT",
  "repository": "hypernym-studio/merge",
  "homepage": "https://github.com/hypernym-studio/merge",
