  "devDependencies": {
    "@hypernym/bundler": "^0.14.3",
    "@hypernym/eslint-config": "^3.6.0",
    "@hypernym/prettier-config": "^3.2.3",
    "@hypernym/tsconfig": "^2.6.0",
